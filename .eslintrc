{
    "extends": ["airbnb","prettier"],
    "plugins":["prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi":2,
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "no-underscore-dangle":[1,{"allow":["_id"]}],
        "linebreak-style":"off"     
            
    }
}